{
  "manifest_version": 3,
  "name": "Simple Test Extension",
  "version": "1.0.0",
  "description": "Simple extension for testing MCP debug functionality",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab"]
}
