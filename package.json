{
  "name": "chrome-debug-mcp",
  "version": "2.0.0",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node build/main.js",
    "start:remote": "node build/remote.js",
    "start:legacy": "node build/index.js",
    "dev": "tsc && node build/main.js",
    "dev:remote": "tsc && node build/remote.js",
    "dev:legacy": "tsc && node build/index.js"
  },
  "keywords": [
    "mcp",
    "chrome",
    "puppeteer",
    "debug"
  ],
  "author": "",
  "license": "MIT",
  "description": "Chrome debug MCP server (Puppeteer + CDP)",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.19.1",
    "chrome-remote-interface": "^0.33.0",
    "puppeteer": "^22.8.2"
  },
  "devDependencies": {
    "@types/chrome-remote-interface": "^0.31.14",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
